    <!-- BEGIN: Content-->
<section id="data-list-view" class="data-list-view-header">
  @if (ViewBag.er != null)
{
    <p class="alert alert-success">@ViewBag.er</p>
}
  <!-- DataTable starts -->
  <div class="table-responsive">
    <table class="table data-list-view">
      <thead>
          <tr>
          <th></th>
          <th>ردیف</th>
          <th>کد فاکتور</th>
          <th>کدکالا</th>
           <th>نام کالا</th>
           <th>تعداد </th>
          <th>قیمت</th>
           <th>قیمت کل</th>
           <th>وضعیت</th>
         
        </tr>
      </thead>
      <tbody>
@if (ViewBag.list != null)
{
  int i=1;
    foreach (var item in ViewBag.list)
    {
          <tr>
            <td></td>
            <td class="product-category">@i</td>
            <td class="product-name">@item.Id</td>
            <td class="product-category">@item.Product_Id </td>
            <td class="product-category">@item.product_Name </td>
            <td class="product-name">@item.Product_Count</td>
            <td class="product-category">@item.product_Price </td>
            <td class="product-category">@item.Total_sum </td>
             @* <td class="product-category"></td> *@
           
        </tr>
        i++;

    }
}
      

       




      </tbody>
    </table>
  </div>
  <!-- DataTable ends -->

  <!-- add new sidebar starts -->
 
  <!-- add new sidebar ends -->
</section>
<!-- Data list view end -->
